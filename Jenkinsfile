pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'Building...'
                echo 'This is from the MavenProjectForJenkins git repo'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Hello World'
            }
        }
    }
}


